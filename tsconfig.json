{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "esnext.asynciterable"
    ],
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "resolveJsonModule": true
  }
}
